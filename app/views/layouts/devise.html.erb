    <![endif]-->
  </head>
  <body class="hold-transition login-page">
    <%= render "partials/alert" %>
    <%= yield %>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA1Sle-FLE8KuTpBvtogMqwXgMCisKj9ME&signed_in=true&libraries=places&callback=initAutocomplete" async defer></script>
